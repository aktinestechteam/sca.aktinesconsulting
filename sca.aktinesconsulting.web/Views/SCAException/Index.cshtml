@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "v";
    ViewData["BreadcrumbHeader"] = "SCA Exception";
    ViewData["BreadcrumbPage"] = "Process";
}

<div class="row">
    <div class="col-md-12">
        <div class="portlet">
            <div class="portlet-header portlet-header-bordered">
                <h3 class="portlet-title">Booking Entry Manual input</h3>
            </div>
            <div class="portlet-body">
                <div class="d-grid gap-3">
                    <div class="col-md-6">
                        <form asp-controller="SCAException" asp-action="ProcessBookingEntry" asp-antiforgery="false"
                              id="fmUpload" method="post"
                              enctype="multipart/form-data">
                            <div class="modal-body" style="padding:10px">
                                <div class="flex-column">
                                    <label for="formFile" class="form-label">Select File</label>
                                    <input class="form-control" type="file" id="formFile" accept=".xlsx">
                                </div>
                            </div>

                            <div class="modal-footer">
                                <button id="btnUpload" type="button" class="btn btn-primary mar" fdprocessedid="3hmaxr" style="margin-right:5px"><i class="fa fa-play-circle" style="margin-right:5px"></i>Process</button>
                                <button id="btnExceptions" type="button" class="btn btn-outline-info" fdprocessedid="igougf" data-bs-toggle="modal" data-bs-target="#modal10"><i class="fa fa-list" style="margin-right:5px" ></i>Exceptions</button>

                            </div>
                        </form>
                    </div>
                </div>
                <hr />
                <div>
                    <table id="dtBookingEntry" class="table table-bordered table-striped table-hover dataTable no-footer dtr-inline">
                        <thead>
                            <tr>
                                <td>AWB</td>
                                <td>BookingCreatedDate</td>
                                <td>FlightDepartureDate</td>
                                <td>BOOKING_TIME</td>
                                <td>FLIGHT_DEP_TIME</td>
                                <td>BookingOrigin</td>
                                <td>BookingDestination</td>
                                <td>BookingRateType</td>
                                <td>CURRENCY_CD</td>
                                <td>AgentCode</td>
                                <td>AgentName</td>
                                <td>BookingDestinationCountry</td>
                                <td>BookingDestinationRegion</td>
                                <td>BookingLastUpdatedBy</td>
                                <td>BookingOriginCountry</td>
                                <td>BookingOriginSalesArea</td>
                                <td>BookingOriginRegion</td>
                                <td>BookingReference</td>
                                <td>Channel</td>
                                <td>SpecialHandlingCodes</td>
                                <td>ChargeableWeight</td>
                                <td>RevGBP</td>
                                <td>Volume</td>
                                <td>ProductCode</td>
                                <td>ProductName</td>
                                <td>FlightNumber</td>
                                <td>MetalInfo</td>
                                <td>ExceptionId</td>
                                <td>ExceptionDescription</td>
                                <td>ExceptionRuleExist</td>
                            </tr>
                        </thead>
                        <tbody></tbody>

                    </table>

                </div>
            </div>
        </div>

    </div>
</div>




@section Styles{
    <style>
        .buttons-colvis {
            margin: 5px;
        }

        .buttons-excel {
            margin: 5px;
        }

        .bg_color {
            background-color: blue !important;
        }
    </style>
}
@section Scripts {

    <script src="~/app/sca-exception.js" asp-append-version="true"></script>


}